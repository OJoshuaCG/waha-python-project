# WAHA FastAPI Environment Variables

# WAHA Server Configuration
# Use http://waha:3000 if running inside Docker Compose
WAHA_URL=http://localhost:3000
WAHA_API_KEY=

# Application Configuration
APP_HOST=0.0.0.0
APP_PORT=8000
APP_DEBUG=true

# Upload Configuration
UPLOAD_DIR=./uploads
MAX_UPLOAD_SIZE=52428800

# Logging
LOG_LEVEL=INFO
